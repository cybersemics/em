{
  "name": "em",
  "brandVersion": "1",
  "version": "76.0.0",
  "author": "Raine Revere <raine@cybersemics.org>",
  "description": "A multi-hierarchical mind-mapping app for personal sensemaking.",
  "license": "NoHarm-draft",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.6",
    "emoji-strip": "^1.0.1",
    "esprima": "^4.0.1",
    "firebase-encode": "0.0.5",
    "he": "^1.2.0",
    "htmlparser2": "^3.10.1",
    "localforage": "^1.7.3",
    "lodash.debounce": "^4.0.8",
    "lodash.flow": "^3.5.0",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.22.2",
    "murmurhash3js": "^3.0.1",
    "pluralize": "^8.0.0",
    "react": "^16.6.3",
    "react-click-n-hold": "^1.0.6",
    "react-contenteditable": "^3.2.2",
    "react-dnd": "^7.6.2",
    "react-dnd-html5-backend": "^7.4.4",
    "react-dnd-multi-backend": "^3.2.1",
    "react-dnd-touch-backend": "^0.8.3",
    "react-dom": "^16.8.6",
    "react-native-web": "^0.11.2",
    "react-redux": "^5.1.1",
    "react-scripts": "^3.3.0",
    "react-transition-group": "^4.2.2",
    "redux": "^4.0.1",
    "static-eval": "^2.0.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@sindresorhus/slugify": "^0.3.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "https://github.com/raineorshine/eslint-loader#patch",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "md5": "^2.2.1",
    "xml2json": "^0.11.2"
  },
  "scripts": {
    "start": "PORT=3333 react-scripts start",
    "build": "react-scripts build",
    "deploy": "npm version major && npm run build && firebase deploy",
    "lint": "eslint src",
    "test": "npm run lint",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": []
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "mocha": true
    },
    "extends": [
      "plugin:react/recommended",
      "standard"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "fp"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "comma-dangle": [
        "off"
      ],
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "operator-linebreak": [
        "off"
      ],
      "padded-blocks": [
        "off"
      ],
      "indent": [
        "off"
      ],
      "space-before-function-paren": [
        "off"
      ],
      "object-curly-newline": [
        "off"
      ],
      "object-property-newline": [
        "off"
      ],
      "quotes": [
        "error",
        "single",
        {
          "allowTemplateLiterals": true
        }
      ],
      "react/prop-types": [
        "off"
      ],
      "react/no-unescaped-entities": [
        "off"
      ],
      "react/no-children-prop": [
        "off"
      ],
      "fp/no-class": [
        "error"
      ],
      "fp/no-delete": [
        "error"
      ],
      "fp/no-events": [
        "error"
      ],
      "fp/no-get-set": [
        "error"
      ],
      "fp/no-let": [
        "error"
      ],
      "fp/no-loops": [
        "error"
      ],
      "fp/no-mutating-assign": [
        "error"
      ],
      "fp/no-mutating-methods": [
        "error"
      ],
      "fp/no-this": [
        "error"
      ],
      "fp/no-valueof-field": [
        "error"
      ]
    }
  }
}
