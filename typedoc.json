{
  "inputFiles": ["src/*"],
  "exclude": ["src/!(@types)/*/*", "src/registerServiceWorker.js", "src/index.js"],
  "excludeExternals": true,
  "mode": "modules",
  "out": "docs",
  "plugin": [
    "typedoc-plugin-external-module-name",
    "typedoc-plugin-exclude-references",
    "typedoc-plugin-rename-named-parameters"
  ],
  "theme": "./node_modules/em-typedoc-theme/bin/default/",
  "readme": "CONTRIBUTING.md",
  "namedParameters": "options"
}
